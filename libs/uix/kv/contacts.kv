<Contacts>:
    name: 'contacts'

    BoxLayout:
        orientation:'vertical'

        ScrollView:
            size_hint_x: None
            width:300
            do_scroll_x: False
            MDList:
                LabelSection:
                    text:'contact1'
                LabelSection:
                    text:'contact2'
                LabelSection:
                    text:'contact3'
                LabelSection:
                    text:'contact4'
                LabelSection:
                    text:'contact5'
        Button:
            color: 1,1,1,1
            background_color:0,0,5,0.8
            front_size: '20sp'
            size_hint_y: None
            height: 100
            text:' find new contact'
            on_release:
                app.show_find_new_contact()
